    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "9.5.0",
    "@nestjs/schematics": "9.2.0",
    "@nestjs/testing": "9.4.2",
