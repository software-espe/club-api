    "jest": "29.5.0",
    "prettier": "2.8.8",
    "prisma": "4.14.1",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.3",
    "ts-node": "10.9.1",
    "ts-prune": "0.10.3",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.0.4"
  }
