    "@types/jest": "29.5.1",
    "@types/node": "18.16.12",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.59.7",
    "@typescript-eslint/parser": "5.59.7",
    "eslint": "8.41.0",
